{
  "name": "<%= name %>",
  "private": true,
  "dependencies": {
  <%- Object.keys(dependencies).sort().map(function(name) {
    return '"'+name+'": "'+dependencies[name]+'"';
  }).join(',\n    '); %>
  },
  "devDependencies": {
    <%- Object.keys(devDependencies).sort().map(function(name) {
      return '"'+name+'": "'+devDependencies[name]+'"';
    }).join(',\n    '); %>
  },
  "browserify": {
    "transform": <%- JSON.stringify(transforms); %>
  },
  "scripts": {
    <%- Object.keys(scripts).sort().map(function(name) {
      return '"'+name+'": "'+scripts[name]+'"';
    }).join(',\n    '); %>
  }
}
